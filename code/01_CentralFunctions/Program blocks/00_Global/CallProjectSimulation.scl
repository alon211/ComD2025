	        _.Unit2.EM3.HmiInterface.cm10SignalTM.commands.slideCylinderWorkPositionDetection1 := FALSE;
	    END_IF;
	    IF Unit2.EM3.HmiInterface.cm7hookClawCylinder1.monitoring.goToHomePos THEN
