      U2EM1cm2SingalTM { S7_SetPoint := 'False'} : "LEXCHANGETRAY_typeRollerIoSignalInterface";
      U2EM2cm1Vfd206 { S7_SetPoint := 'False'} : "LEXCHANGETRAY_typeRollerVfdInterface";
      U2EM2cm1Vfd : "LEXCHANGETRAY_typeVfdInterface";
