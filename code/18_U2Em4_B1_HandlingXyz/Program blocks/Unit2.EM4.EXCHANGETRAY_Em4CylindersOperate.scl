	    ;
	    
	    // Check if all enabled cylinders are in work position
	    // æ£€æŸ¥æ‰€æœ‰ä½¿èƒ½çš„æ°”ç¼¸æ˜¯å¦éƒ½åœ¨å·¥ä½œä½ç½®
	    #workDone :=
	    #cm4OnWork
	    AND #cm5OnWork
	    AND #cm6OnWork
	    AND #cm7OnWork
	    ;
	END_REGION PROGRAM LOGIC
	
	REGION OUTPUTS
	    // ENO mechanism for error handling
	    // ENOæœºåˆ¶ç”¨äºŽé”™è¯¯å¤„ç†
	    ENO := TRUE;
	END_REGION OUTPUTS
END_FUNCTION
